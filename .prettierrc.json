{
	"singleQuote": false,
	"printWidth": 200,
	"endOfLine": "lf",
	"tabWidth": 4,
	"useTabs": true,
	"overrides": [
		{
			"files": ["*.yaml", "*.yml", "*.md", "*.json", "*.xml", "*.properties", "*.ts", "*.tsx"],
			"options": {
				"useTabs": true,
				"tabWidth": 2,
				"keySeparator": "=",
				"xmlWhitespaceSensitivity": "ignore"
			}
		}
	]
}
